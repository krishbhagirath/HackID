generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "rhel-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model hackathons {
  hackathon_id String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  org_id       String
  name         String
  devpost_url  String     @unique
  start_time   DateTime   @db.Timestamptz(6)
  end_time     DateTime   @db.Timestamptz(6)
  created_at   DateTime   @default(now()) @db.Timestamptz(6)
  projects     projects[]
}

model projects {
  project_id       String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  hackathon_id     String     @db.Uuid
  source_url       String     @unique
  title            String?
  github_repo_link String?
  data             Json?
  created_at       DateTime   @default(now()) @db.Timestamptz(6)
  hackathons       hackathons @relation(fields: [hackathon_id], references: [hackathon_id], onDelete: Cascade, onUpdate: NoAction)
}
